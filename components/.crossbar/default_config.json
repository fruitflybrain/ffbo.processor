{
    "workers":[
        {
            "type":"router",
            "transports":[
                {
                    "paths":{
                        "ws":{
                            "type":"websocket",
                            "auth":{
                                "wampcra":{
                                    "authenticator":"ffbo.auth_server.auth",
                                    "type":"dynamic"
                                },
                                "anonymous":{
                                    "type":"static",
                                    "role":"anonymous"
                                }
                            },
                            "options":{
                                "enable_webstatus": false,
                                "max_frame_size": 0,
                                "max_message_size": 0,
                                "auto_fragment_size": 65536,
                                "fail_by_drop": true,
                                "open_handshake_timeout": 2500,
                                "close_handshake_timeout": 1000,
                                "auto_ping_interval": 10000,
                                "auto_ping_timeout": 0,
                                "auto_ping_size": 4,
                                "compression": {
                                    "deflate": {
                                        "request_no_context_takeover": false,
                                        "request_max_window_bits": 11,
                                        "no_context_takeover": false,
                                        "max_window_bits": 11,
                                        "memory_level": 4
                                    }
                                }
                            }
                        },
                        "/":{
                            "directory":"../web/ffbo.neuronlp",
                            "type":"static"
                        }
                    },
                    "endpoint":{
                        "tls":{
                            "dhparam":"dhparam",
                            "certificate":"",
                            "key":"",
			    "chain_certificates": [
				""
			    ]
                        },
                        "type":"tcp",
                        "port":8081
                    },
                    "type":"web"
                },
                {
                    "paths":{
                        "ws":{
                            "type":"websocket",
                            "auth":{
                                "wampcra":{
                                    "authenticator":"ffbo.auth_server.auth",
                                    "type":"dynamic"
                                },
                                "anonymous":{
                                    "type":"static",
                                    "role":"anonymous"
                                }
                            }
                        },
                        "/":{
                            "directory":"../web/ffbo.neurogfx",
                            "type":"static"
                        }
                    },
                    "endpoint":{
                        "tls":{
                            "dhparam":"dhparam",
                            "certificate":"",
                            "key":"",
			    "chain_certificates": [
				""
			    ]
                        },
                        "type":"tcp",
                        "port":8082
                    },
                    "type":"web"
                },
                {
                    "paths":{
                        "ws":{
                            "type":"websocket",
                            "auth":{
                                "wampcra":{
                                    "authenticator":"ffbo.auth_server.auth",
                                    "type":"dynamic"
                                },
                                "anonymous":{
                                    "type":"static",
                                    "role":"anonymous"
                                }
                            }
                        },
                        "/":{
                            "directory":"../web/sandbox",
                            "type":"static"
                        }
                    },
                    "endpoint":{
                        "tls":{
                            "dhparam":"dhparam",
                            "certificate":"",
                            "key":"",
			    "chain_certificates": [
				""
			    ]
                        },
                        "type":"tcp",
                        "port":8083
                    },
                    "type":"web"
                }
            ],
            "options":{
                "pythonpath":[
                    ".."
                ]
            },
            "realms":[
                {
		    "name": "realm1",
		    "roles":[
                        {
                            "name":"anonymous",
                            "permissions":[
                                {
                                    "cache":true,
                                    "uri":"ffbo.auth_server.register_user",
                                    "allow":{
                                        "subscribe":false,
                                        "register":false,
                                        "call":true,
                                        "publish":false
                                    },
                                    "match":"exact"
                                },
                                {
                                    "cache":true,
                                    "uri":"ffbo.auth_server.user_exists",
                                    "allow":{
                                        "subscribe":false,
                                        "register":false,
                                        "call":true,
                                        "publish":false
                                    },
                                    "match":"exact"
                                }
                            ]
                        },
                        {
                            "name":"user",
                            "permissions":[
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.ui",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":false,
                                        "publish":false
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.gfx",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":false,
                                        "publish":false
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"",
                                    "allow":{
                                        "subscribe":true,
                                        "register":false,
                                        "call":true,
                                        "publish":false
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "cache":true,
                                    "uri":"ffbo.server.register",
                                    "allow":{
                                        "subscribe":false,
                                        "register":false,
                                        "call":false,
                                        "publish":false
                                    },
                                    "match":"exact"
                                },
                                {
                                    "cache":true,
                                    "uri":"ffbo.auth_server.auth",
                                    "allow":{
                                        "subscribe":false,
                                        "register":false,
                                        "call":false,
                                        "publish":false
                                    },
                                    "match":"exact"
                                },
                                {
                                    "cache":true,
                                    "uri":"ffbo.auth_server.get_user",
                                    "allow":{
                                        "subscribe":false,
                                        "register":false,
                                        "call":false,
                                        "publish":false
                                    },
                                    "match":"exact"
                                }
                            ]
                        },
                        {
                            "name":"components",
                            "permissions":[
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.na",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":false,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.nlp",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.nk",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":false,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.gfx",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                },
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"",
                                    "allow":{
                                        "subscribe":true,
                                        "register":false,
                                        "call":true,
                                        "publish":false
                                    },
                                    "match":"prefix"
                                }
                            ]
                        },
                        {
                            "name":"processor",
                            "permissions":[
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                }
                            ]
                        },
                        {
                            "name":"authenticator",
                            "permissions":[
                                {
                                    "disclose":{
                                        "publisher":true,
                                        "caller":true
                                    },
                                    "cache":true,
                                    "uri":"ffbo.auth_server",
                                    "allow":{
                                        "subscribe":true,
                                        "register":true,
                                        "call":true,
                                        "publish":true
                                    },
                                    "match":"prefix"
                                }
                            ]
                        }
                    ],
                    "name":"realm1",
                    "store":{
                        "call-queue":[
                            {
                                "limit":1000,
                                "uri":"ffbo.nlp.query",
                                "match":"prefix"
                            },
                            {
                                "limit":1000,
                                "uri":"ffbo.nk",
                                "match":"prefix"
                            },
                            {
                                "limit":1000,
                                "uri":"ffbo.na.query_to_processor",
                                "match":"prefix"
                            },
			    {
				"uri": "ffbo.na",
				"match": "prefix",
				"limit": 1000
			    },
			    {
				"uri": "ffbo.processor.fetch_flycircuit",
				"match": "prefix",
				"limit": 1000
			    }

                        ],
                        "type":"memory"
                    }
                }
            ],
            "components":[
                {
                    "classname":"processor_component.processor_component.AppSession",
                    "role":"processor",
                    "realm":"realm1",
                    "type":"class"
                },
                {
                    "classname":"processor_component.authenticator.AppSession",
                    "role":"authenticator",
                    "realm":"realm1",
                    "type":"class"
                }
            ]
        }
    ],
    "version":2
}
